{
    "datasets": [
        {
            "name": "experimental-set2",
            "filter": {
                "exclude": [{
                    "modifier": ["native"]
                },{
                    "label": ["testCode", "override"]
                }]
            },
            "isGroupOnly": true,
            "specialOptions": {
                "convert2And4ToWords": true,
                "minMethodNameLength": 2,
                "datasetSize": {
                    "training": 7,
                    "evaluation": 3
                },
                "maxTrainingRows": 4000,
                "maxEvaluationRows": 3000
            },
            "modelOptions": {
                "numOfEpochs": 1
            },
            "subsets": [{
                "name": "withoutPrivates",
                "filter":{
                    "exclude": {
                        "modifier":["private"]
                    }
                },
                "specialOptions": {
                    "maxTrainingRows": 4000,
                    "maxEvaluationRows": 3000
                }
            }, {
                "name": "withoutNumbers",
                "filter":{
                    "exclude": {
                        "method": [{
                            "pattern": "[0-9_]",
                            "type": "regexp"
                        }]
                    }
                },
                "specialOptions": {
                    "maxTrainingRows": 4000,
                    "maxEvaluationRows": 3000
                }
            }]
        }
    ]
}