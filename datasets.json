{
    "datasets": [
        {
            "name": "exp-sentences-220405",
            "filter": {
                "exclude": [{
                    "modifier": ["native", "private"]
                },{
                    "label": ["testCode", "override"]
                }]
            },
            "isGroupOnly": true,
            "specialOptions": {
                "convert2ToWords": true,
                "minMethodNameLength": 2,
                "datasetSize": {
                    "training": 7,
                    "evaluation": 3
                },
                "maxTrainingRows": 25000,
                "maxEvaluationRows": 8000,
                "sentenceFormatting": true
            },
            "modelOptions": {
                "numOfEpochs": 1,
                "generationTasks": {
                    "parameterNames":{
                        "withParameterTypes": true,
                        "withReturnType": true
                    }
                }
            }
        }
    ]
}